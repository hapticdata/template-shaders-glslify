{
  "name": "template-shader",
  "version": "1.0.1",
  "description": "A boilerplate for playing with shaders in webgl",
  "main": "javascripts/index.js",
  "author": "Kyle Phillips <kyle@haptic-data.com> (http://hapticdata.com/)",
  "browserify": {
    "transform": [
      "glslify"
    ]
  },
  "scripts": {
    "build": "browserify javascripts/index.js > public/bundle.js",
    "dev": "budo javascripts/index.js:bundle.js --host=localhost -v --dir=public | garnish"
  },
  "license": "MIT",
  "dependencies": {
    "animitter": "^3.0.0",
    "browserify": "^16.2.2",
    "budo": "^11.3.2",
    "garnish": "^5.2.0",
    "gl-shader": "^4.2.1",
    "glsl-hsl2rgb": "^1.1.0",
    "glsl-noise": "0.0.0",
    "glsl-shader-name": "^1.0.0",
    "glslify": "^6.1.1",
    "lodash": "^4.17.10"
  },
  "devDependencies": {}
}
